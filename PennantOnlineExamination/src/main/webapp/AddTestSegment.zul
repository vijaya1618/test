<zk>
<window closable="true" id="AddTestSegmentWin" use="com.pennantExamination.controller.AddTestSegmentController" >
<style>
.z-column {
background:#f57f20;
border-left: 1px solid #f57f20;
border-bottom: 1px solid #f57f20;
}
.z-grid-header-border {
border bottom:none;
border-bottom: 1px solid #f57f20;
}
.z-grid-body {
width:px;
}

.z-hlayout-inner {
    width: 70%;
}
.z-button {
background-color: #f57f20;
}
.wrapper {
    text-align: end;
}
.wrapperr {
    text-align: start;
}
.button {
    position: absolute;
    top: 50%;
}

</style>

	<hlayout >
		
		
			<grid id="AddTestSegmentgrid" width="1194px">
				<columns>
					<column label="Add TestSegment Details" />
					<column />
				</columns>
				<rows>
					<row>
						Section Name:
						<hlayout>
							<textbox tabindex="1" id="addTestName" width="200px"
								sclass="blue" />
						</hlayout>
					</row>
					<row>
						Enabled:
						<hlayout>
							<checkbox tabindex="2" id="enabled"
								sclass="blue" />
						</hlayout>
					</row>
					<row>
						<cell class="wrapper" colspan="2" width="120px">
							<button label="Add" id="AddSubmit" onClick="AddTestSegmentWin.insertTestSegmentDetails()"/>
						</cell>
						
						<cell class="wrapperr" colspan="2" width="120px">
							<button label="Close" id="editClose" onClick="AddTestSegmentWin.detach()"  />
						</cell>
						<cell></cell>
					</row>
				</rows>
			</grid>
		
	</hlayout>
	</window>
</zk>
